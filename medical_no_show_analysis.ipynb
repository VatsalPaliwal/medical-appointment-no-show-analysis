   "source": [
    "plt.figure(figsize=(10,6))\n",
    "max_age=data['Age'].max()\n",
    "bins = list(np.arange(0, 95, 5)) + [data['Age'].max() + 1]\n",
    "labels = [f\"{b}-{b+5}\" for b in bins[:-2]] + [\"95+\"]\n",
    "\n",
    "overall_no_show_pct = (data['No-show'].value_counts(normalize=True)['Yes']) * 100\n",
    "\n",
    "data[\"AgeRange\"] = pd.cut(data[\"Age\"], bins=bins, labels=labels, right=False)\n",
    "counts = data.groupby([\"AgeRange\", \"No-show\"]).size().unstack(fill_value=0)\n",
    "counts[\"NoShowPct\"] = (counts[\"Yes\"] / (counts[\"Yes\"] + counts[\"No\"])) * 100\n",
    "x = np.arange(len(counts))  \n",
    "# Plot no-show percentage\n",
    "plt.figure(figsize=(14,7))\n",
    "plt.bar(counts.index.astype(str), counts[\"NoShowPct\"])\n",
    "\n",
    "plt.xlabel(\"Age Range (Years)\")\n",
    "plt.ylabel(\"No-show Percentage (%)\")\n",
    "plt.title(\"No-show Percentage by Age Range\")\n",
    "plt.axhline(\n",
    "    y=overall_no_show_pct,\n",
    "    linestyle='--',\n",
    "    linewidth=2,\n",
    "    label=f'Overall No-show Rate ({overall_no_show_pct:.1f}%)',\n",
    "    color='red'\n",
    ")\n",
    "plt.xticks(rotation=45)\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
